package mountain;

public class Side {
	private Point point1;
	private Point point2;
	
	public Side(Point point1, Point point2) {
		this.point1 = point1;
		this.point2 = point2;
	}
	
	@Override
	public boolean equals(Object obj) {
		if(obj instanceof Side) {
			Side side = (Side) obj;
			return side.point1.equals(side.point2);
		}
		
		return false;
	}
	
	@Override
	public int hashCode() {
		return point1.hashCode() + point2.hashCode();
	}
}
